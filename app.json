{
  "expo": {
    "name": "MovePay",
    "slug": "movepay-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "movepay",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.movepay.mobile"
    },
    "android": {
      "package": "com.movepay.mobile",
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": false
        }
      ],
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que $(PRODUCT_NAME) acesse sua c√¢mera para escanear bilhetes",
          "microphonePermission": "Permitir que $(PRODUCT_NAME) acesse o microfone (opcional)",
          "recordAudioAndroid": false
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "84b659c6-2517-4a24-86ba-1d9a124070b5"
      }
    }
  }
}
